# OpenAI-compatible endpoint
# OpenAI:
# VISION_ENDPOINT=https://api.openai.com/v1/chat/completions
# VISION_MODEL=gpt-4o

# Groq:
VISION_ENDPOINT=https://api.groq.com/openai/v1/chat/completions
VISION_MODEL=meta-llama/llama-4-scout-17b-16e-instruct

# Your API key (keep private!)
VISION_API_KEY=YOUR_KEY_HERE

# Optional tuning (safe defaults)
MAX_WORKERS=1
MAX_RETRIES=3
RETRY_DELAY=10
REQUEST_TIMEOUT=120

# Rate limiting (helps avoid 429s)
RATE_LIMIT_RPM=30
RATE_JITTER_MS=200
